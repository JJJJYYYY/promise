{
  "name": "jjj-promise",
  "version": "1.0.3",
  "main": "dist/promise.min.js",
  "repository": "https://github.com/JJJYY/promise.git",
  "author": "JJJYY<jessey9527@gmail.com>",
  "license": "MIT",
  "keyword": [
    "promise",
    "a+",
    "aplus"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run flow && npm run lint",
      "commit-msg": "node scripts/verify-commit-msg.js"
    }
  },
  "scripts": {
    "pretest": "npm run build",
    "prepublish": "npm run flow && npm run lint && npm run build",
    "build": "node build/build.js",
    "dev": "node build/dev.js",
    "flow": "flow check",
    "lint": "eslint src --fix",
    "test": "cross-env NODE_ENV=test mocha --require @babel/register --file test/*.test.js",
    "test:watch": "npm run test -w",
    "release": "standard-version"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "chai": "^4.1.2",
    "chalk": "^2.4.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.5.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "flow-bin": "^0.80.0",
    "husky": "^1.0.0-rc.15",
    "mocha": "^5.2.0",
    "promises-aplus-tests": "^2.1.2",
    "rollup": "^0.65.2",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-eslint": "^5.0.0",
    "rollup-plugin-flow": "^1.1.1",
    "standard-version": "^4.4.0",
    "uglify-js": "^3.4.9",
    "zlib": "^1.0.5"
  }
}
